{
  "name": "commando-boilerplate",
  "version": "0.1.0",
  "description": "Discord.js and Commando starter kit!",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "node --trace-warnings -r dotenv/config ./node_modules/nodemon/bin/nodemon --exec babel-node ./src/index.js",
    "build": "babel ./src -d ./dist",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "npm run lint -- --fix",
    "deps": "npm-check -u",
    "snyk": "snyk test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurdism/commando-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/nurdism/commando-boilerplate/issues"
  },
  "keywords": [
    "boilerplate",
    "commando",
    "discord",
    "bot"
  ],
  "author": "Nurdism <https://github.com/nurdism>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "discord.js": "^11.3.2",
    "discord.js-commando": "github:discordjs/commando#11.2",
    "sequelize": "^4.37.4",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "sqlite3": "^4.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.6.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.17.2",
    "npm-check": "^5.6.0",
    "snyk": "^1.70.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
